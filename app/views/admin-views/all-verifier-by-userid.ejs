     <table class="paginate_table">
        <div class="pagination-list for-ava">
        <% if (verifier_list.data.totalPages> 0) { %>
        
        <ul class="pagination text-center">
          <% if (verifier_list.data.currentPage == 1) { %>
          <li class="disabled"><a>First &nbsp;</a></li>
          <% } else { %>
          <li><a href="/all-verifier-by-userid?page=1&id=<%=reg_user_id %>">First &nbsp;</a></li>
          <% } %>
          <% var i = (Number(verifier_list.data.currentPage) > 5 ? Number(verifier_list.data.currentPage) - 4 : 1) %>
          <% if (i !== 1) { %>
          <li class="disabled"><a>...</a></li>
          <% } %>
          <% for (; i <= (Number(verifier_list.data.currentPage) + 4) && i < verifier_list.data.totalPages; i++) { %>
          <% if (i == verifier_list.data.currentPage) { %>
            
            <li class="active"><a><%= i %> &nbsp;</a></li>
          <% } else { %>
            <li><a href="/all-verifier-by-userid?page=<%= i %>&id=<%=reg_user_id %>"><%= i %> &nbsp;</a></li>
          <% } %>
          <% if (i == Number(verifier_list.data.currentPage) + 4 && i <= verifier_list.data.totalPages) { %>
            <li class="disabled"><a>...</a></li>
          <% } %>
          <% } %>
          <% if (verifier_list.data.currentPage ==verifier_list.data.totalPages) { %>
          <li class="disabled"><a>Next</a></li>
          <% } else { %>
          <li><a href="/all-verifier-by-userid?page=<%=market_list_result.totalPages %>&id=<%=reg_user_id %>"> &gt;Next</a></li>
          <% } %>
        </ul>
        <% } %>
        </div>
      </table>
