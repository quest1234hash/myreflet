<!--                 </div>
 -->            <div class="pagination-list for-user">
          
            <% if (complaint_list.totalPages > 0) { %>
             <ul class="pagination text-center">
               <% if (complaint_list.currentPage == 1) { %>
                 <li class="disabled page-item"><a class="page-link">&lt;</a></li>
               <% } else { %>
                 <li class="page-item"><a class="page-link" href="/complaint-list?page=1">&lt;</a></li>
               <% } %>
          
          
          
               <% var i = (Number(complaint_list.currentPage) > 5 ? Number(complaint_list.currentPage) - 4 : 1) %>
               <% if (i !== 1) { %>
                 <!-- <li class="disabled"><a>...</a></li> -->
                 <li class="page-item disabled"><a class="page-link" >...</a></li>
               <% } %>
               <% for (; i <= (Number(complaint_list.currentPage) + 4) && i <= complaint_list.totalPages; i++) { %>
                 <% if (i == complaint_list.currentPage) { %>
                   <!-- <li class="active"><a><%= i %></a></li> -->
                   <li class="page-item active">
                   <a class="page-link" href="#"><%= i %><span class="sr-only">(complaint_list.currentPage)</span></a>
                   </li>
                 <% } else { %>
                   <li class="page-item"><a class="page-link" href="/complaint-list?page=<%= i %>"><%= i %></a></li>
                 <% } %>
                 <% if (i == Number(complaint_list.currentPage) + 4 && i < complaint_list.totalPages) { %>
                   <!-- <li class="disabled"><a>...</a></li> -->
                   <li class="page-item disabled"><a class="page-link" >...</a></li>
                 <% } %>
               <% } %>
          
          
               <% if (complaint_list.currentPage == complaint_list.totalPages) { %>                       
                 <li class="disabled page-item"><a class="page-link">&gt;</a></li>
               <% } else { %>
                 <li><a class="page-link" href="/complaint-list?page=<%= complaint_list.totalPages %>">&gt;</a></li>
               <% } %>
          
             </ul>
          <% } %>
          
