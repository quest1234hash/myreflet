<html>
<head>
<style>
table {
  border-collapse: collapse;
  width: 100%;
}

th, td {
  text-align: left;
  padding: 8px;
}

tr:nth-child(even){background-color: #f2f2f2}

th {
  background-color:#88beda ;
  color: white;
}
</style>
</head>
<body>
  <div style="font-family: sans-serif;color: #274794;    ">
    <h3 style="margin: 10px 0;font-weight: 500;">Report Name : <%=report_data[0].report_name%></h3>
    <h3 style="margin: 10px 0;font-weight: 500;">Descriptions : <%=report_data[0].description%></h3>
    <h3 style="margin: 10px 0;font-weight: 500;">Start time: <%=moment(report_data[0].start_date).format('MMM DD, YYYY')%></h3>
    <h3 style="margin: 10px 0;font-weight: 500;">End time:<%=moment(report_data[0].end_date).format('MMM DD, YYYY')%>
    </h3>
  </div>
<div class="personal-info-box table-responsive another-table" style="font-family: sans-serif;">
        <table class="table" id="myTableData">
            <thead>
                <tr> 
                    <%if(checked_array.includes('rep_emailid')){%>
                      <!-- <th width="25%" scope="col">Email</th> -->
                     <%}
                     if(checked_array.includes('reflectid_by')){%> 
                      <th width="20%" scope="col">MyReflet Type</th>
                      <%}
                     if(checked_array.includes('rep_firstname')){%> 
                      <th width="15%" scope="col">Name</th>
                      <%}
                     if(checked_array.includes('reflect_code')){%> 
                      <th width="10%" scope="col">MyReflet Code</th>
                      <%}
                      if(checked_array.includes('invite_status')){%> 
                        <th width="15%" scope="col">Status</th>
                      <%}
                      if(checked_array.includes('country')){%> 
                        <th width="10%" scope="col">Country</th>
                    <%}%>
                      <!-- <th width="15%" scope="col">Status</th> -->
                </tr>
              </thead>
            <tbody id="myTable3">
                <%if(report_data_type.length!=0){
                for(let sub_verifier of report_data_type){    
                %>
                <tr>
                    <%if(checked_array.includes('rep_emailid')){%>
                    <!-- <td class="upper"><%=sub_verifier.email%></td> -->
                    <%}if(checked_array.includes('reflectid_by')){%> 
                      <td class="upper"><%=sub_verifier.reflectid_by%></td>
                      <%}if(checked_array.includes('rep_firstname')){%> 
                        <td>
                          
                        <%if(sub_verifier.rep_firstname){%>
                                      <span class="font-w-600"><%=sub_verifier.rep_firstname%></span>
                              <%}else if(sub_verifier.entity_company_name){%>
                                  <span class="font-w-600"> <%=sub_verifier.entity_company_name%> </span>
      
                              <%}else{%>
                                  <span class="font-w-600"> <%=decrypt(sub_verifier.full_name)%> </span>
                              <%}%>



                        </td>             
                   <%}if(checked_array.includes('reflect_code')){%> 
                    <td><%=sub_verifier.reflect_code%></td>
                    <%}%>
                    <%if(checked_array.includes('invite_status')){%> 
                        <td>
                            <%if(sub_verifier.invite_status=="pending"){%>
                            <div class="pending-status">
                                Pending
                            </div>
                            <%}%>
                            <%if(sub_verifier.invite_status=="reject"){%>
                                <div class="cancel-status">
                                    Reject
                                </div>
                            <%}%>
                            <%if(sub_verifier.invite_status=="accept"){%>
                                    <div class="approved-status">
                                        Accept
                                    </div>
                            <%}%>

                        </td> 
                                                <%}%>
                    <%if(checked_array.includes('country')){%> 
                    <td class="upper"><%if(sub_verifier.country_name){%>
                        <%=sub_verifier.country_name%>
                        <%}else{%>
                        -
                        <%}%></td>						
                    <%}%>
                

                </tr> 
                <%}}else{
     %>
                       <tr>
            <td colspan="6"><center>No record found.</center> </td>
             </tr>       <%}%>
            </tbody>
      </div>
</body>
</html>
